   "source": [
    "def get_layered_chart(chart_one: alt.Chart, chart_two: alt.Chart, randomly_chosen_column: str, country: str) -> alt.Chart:\n",
    "    return alt.layer(\n",
    "        chart_one,\n",
    "        chart_two\n",
    "    ).resolve_scale(\n",
    "        y=\"independent\",\n",
    "        color=\"independent\"\n",
    "        ).properties(\n",
    "        title=f'{randomly_chosen_column} and Eurovision Place by Year in {country}',\n",
    "        width=1000\n",
    "    )"
   ]
  },
